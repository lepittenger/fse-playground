				},
				{
					"slug": "tint-t2",
					"color": "#81C784",
					"name": "Tint/T2"
				},
				{
					"slug": "tint-t3",
					"color": "#A5D6A7",
					"name": "Tint/T3"
				},
				{
					"slug": "tint-t4",
					"color": "#C8E6C9",
					"name": "Tint/T4"
				},
				{
					"slug": "tint-t5",
					"color": "#E8F5E9",
					"name": "Tint/T5"
				},
				{
					"slug": "action-warning",
					"color": "#FBC02D",
					"name": "Action/Warning"
				},
				{
					"slug": "action-error",
					"color": "#E53835",
					"name": "Action/Error"
				},
				{
					"slug": "action-success",
					"color": "#2E7D31",
					"name": "Action/Success"
				}
			]
		},
		"layout": {
			"contentSize": "700px",
			"wideSize": "1100px"
		},
		"spacing": {
			"units": [ "px", "em", "rem", "vh", "vw", "%" ]
		},
		"typography": {
			"customFontSize": false,
			"dropCap": false,
			"fluid": true,
			"fontStyle": false,
			"fontFamilies": [
				{
					"fontFamily": "Inter, sans-serif",
					"name": "Inter",
					"slug": "sans",
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Inter",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400 500 600 700",
							"src": ["file:./assets/fonts/Inter.var.woff2"]
						}
					]
				},
				{
					"fontFamily": "monospace",
					"name": "Monospace",
					"slug": "monospace"
				}
			],
			"fontSizes": [
				{
					"slug": "body-4",
					"size": "0.75rem",
					"name": "Body 4",
					"fluid": false
				},
				{
					"slug": "body-3",
					"size": "0.875rem",
					"name": "Body 3",
					"fluid": false
				},
				{
					"slug": "body-2",
					"size": "1rem",
					"name": "Body 2",
					"fluid": false
				},
				{
					"slug": "body-1",
					"size": "1.125rem",
					"name": "Body 1",
					"fluid": false
				},
				{
					"slug": "headline-4",
					"size": "1.25rem",
					"name": "Headline 4",
					"fluid": false
				},
				{
					"slug": "headline-3",
					"size": "1.75rem",
					"name": "Headline 3",
					"fluid": false
				},
				{
					"slug": "headling-2",
					"size": "2.25rem",
					"name": "Headline 2",
					"fluid": false
				},
				{
					"slug": "headline-1",
					"size": "4rem",
					"name": "Headline 1",
					"fluid": false
				}
			],
			"letterSpacing": false,
			"lineHeight": false,
			"textTransform": false,
			"textDecoration": false
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/code": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				}
			},
			"core/comment-author-name": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-edit-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-reply-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/navigation": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "700"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--contrast)"
						},
						"typography": {
							"textDecoration": "underline"
						},
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--contrast)"
							},
							"typography": {
								"textDecoration": "none"
							}
						},
						":focus": {
							"color": {
								"text": "var(--wp--preset--color--contrast)"
							}
						},
						":active": {
							"color": {
								"text": "var(--wp--preset--color--contrast)"
							}
						}
					}
				}
			},
			"core/post-author": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--extra-small)"
				}
			},
			"core/post-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--extra-small)"
				}
			},
			"core/post-navigation-link": {
				"typography": {
					"fontWeight": "700"
				}
			},
			"core/post-terms": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--extra-small)"
				}
			},
			"core/post-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"core/query-pagination": {
				"typography": {
					"fontWeight": "700"
				}
			},
			"core/query-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"core/site-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"elements": {
			"button": {
				"border": {
					"color": "var(--wp--preset--color--transparent)",
					"width": "4px",
					"style": "solid",
					"radius": "4px"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--contrast)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "700"
				},
				"shadow": "var(--wp--preset--shadow--natural)",
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--contrast)"
					},
					"border": {
						"color": "var(--wp--preset--color--secondary)"
					}
				}
			},
			"caption": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			},
			"cite": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--contrast)"
					},
					"typography": {
						"textDecoration": "none"
					}
				},
				":focus": {
					"color": {
						"text": "var(--wp--preset--color--contrast)"
					}
				},
				":active": {
					"color": {
						"text": "var(--wp--preset--color--contrast)"
					}
				}
			}
		},
		"spacing": {
			"padding": {
				"right": "var(--wp--preset--spacing--50)",
				"left": "var(--wp--preset--spacing--50)"
			}
		},
		"typography": {
			"fontSize": "var(--wp--preset--font-size--medium)",
			"fontFamily": "var(--wp--preset--font-family--sans)",
			"lineHeight": "1.75"
		}
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		}
	],
	"customTemplates": [
		{
			"name": "no-title",
			"title": "No title",
			"postTypes": [ "post","page" ]
		}
	]
}
