{
	"version": 2,
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"settings": {
		"appearanceTools": true,
		"blocks": {
			"core/heading":{
				"color": {
				 "palette": [
					 {
						 "slug": "tertiary",
						 "color": "#1d1f35",
						 "name": "Tertiary"
					 }
				 ]
				}
			}
		},
		"color": {
			"palette": [
				{
					"slug": "neutral-black",
					"color": "#263238",
					"name": "Neutral/Black"
				},
				{
					"slug": "neutral-d-grey",
					"color": "#4D4D4D",
					"name": "Neutral/D_Grey"
				},
				{
					"slug": "neutral-grey",
					"color": "#717171",
					"name": "Neutral/Grey"
				},
				{
					"slug": "neutral-l-grey",
					"color": "#89939E",
					"name": "Neutral/L_Grey"
				},
				{
					"slug": "neutral-grey-blue",
					"color": "#ABBED1",
					"name": "Neutral/Grey_Blue"
				},
				{
					"slug": "neutral-silver",
					"color": "#F5F7FA",
					"name": "Neutral/Silver"
				},
				{
					"slug": "neutral-white",
					"color": "#FFFFFF",
					"name": "Neutral/White"
				},
				{
					"slug": "brand-primary",
					"color": "#4CAF4F",
					"name": "Brand/Primary"
				},
				{
					"slug": "brand-secondary",
					"color": "#263238",
					"name": "Brand/Secondary"
				},
				{
					"slug": "brand-info",
					"color": "#2194F3",
					"name": "Brand/Info"
				},
				{
					"slug": "shade-s1",
					"color": "#43A046",
					"name": "Shade/S1"
				},
				{
					"slug": "shade-s2",
					"color": "#388E3B",
					"name": "Shade/S2"
				},
				{
					"slug": "shade-s3",
					"color": "#237D31",
					"name": "Shade/S3"
				},
				{
					"slug": "shade-s4",
					"color": "#1B5E1F",
					"name": "Shade/S4"
				},
				{
					"slug": "shade-s5",
					"color": "#103E13",
					"name": "Shade/S5"
				},
				{
					"slug": "tint-t1",
					"color": "#66BB69",
					"name": "Tint/T1"
				},
				{
					"slug": "tint-t2",
					"color": "#81C784",
					"name": "Tint/T2"
				},
				{
					"slug": "tint-t3",
					"color": "#A5D6A7",
					"name": "Tint/T3"
				},
				{
					"slug": "tint-t4",
					"color": "#C8E6C9",
					"name": "Tint/T4"
				},
				{
					"slug": "tint-t5",
					"color": "#E8F5E9",
					"name": "Tint/T5"
				},
				{
					"slug": "action-warning",
					"color": "#FBC02D",
					"name": "Action/Warning"
				},
				{
					"slug": "action-error",
					"color": "#E53835",
					"name": "Action/Error"
				},
				{
					"slug": "action-success",
					"color": "#2E7D31",
					"name": "Action/Success"
				}
			],
			"gradients": [
				{
					"slug": "primary-white",
					"gradient": "linear-gradient(#0073aa 49.9%,#fff 50%)",
					"name": "Primary color to white"
				},
				{
					"slug": "white-primary",
					"gradient": "linear-gradient(#fff 49.9%,#0073aa 50%)",
					"name": "Horizontal white to Primary color"
				}
			],
			"duotone": [
				{
					"colors": [ "#00a0d2", "#0073aa"],
					"slug": "secondary-primary",
					"name": "Secondary and primary"
				},
				{
					"colors": ["#0073aa", "#f0f0f0"],
					"slug": "primary-contrast",
					"name": "Primary and Contrast"
				}
			]
		},
		"layout": {
			"contentSize": "700px",
			"wideSize": "1100px"
		},
		"spacing": {
			"units": [ "px", "em", "rem", "vh", "vw", "%" ]
		},
		"typography": {
			"dropCap": true,
			"fluid": true,
			"fontFamilies": [
				{
					"name": "System",
					"slug": "system",
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif"
				},
				{
					"name": "Serif",
					"slug": "serif",
					"fontFamily": "\"Times New Roman\",\"New York\",Times,\"Noto Serif\",serif"
				},
				{
					"name": "Monospace",
					"slug": "monospace",
					"fontFamily": "Consolas,Menlo,Monaco,\"SF Mono\",\"DejaVu Sans Mono\",\"Roboto Mono\",\"Courier New\",Courier,monospace"
				}
			],
			"fontSizes": [
				{
					"slug": "extra-small",
					"size": "1rem",
					"name": "Extra small",
					"fluid": false
				},
				{
					"slug": "small",
					"size": "1.125rem",
					"name": "Small",
					"fluid": false
				},
				{
					"slug": "medium",
					"size": "1.25rem",
					"name": "Medium",
					"fluid": {
						"min": "1.25rem",
						"max": "1.5rem"
					}
				},
				{
					"slug": "large",
					"size": "2rem",
					"name": "Large",
					"fluid": {
						"min": "1.75rem",
						"max": "2rem"
					}
				},
				{
					"slug": "x-large",
					"size": "2.75rem",
					"name": "XL",
					"fluid": {
						"min": "2.5rem",
						"max": "2.75rem"
					}
				},
				{
					"slug": "xx-large",
					"size": "3.75rem",
					"name": "XXL",
					"fluid": {
						"min": "3rem",
						"max": "3.75rem"
					}
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/code": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				}
			},
			"core/comment-author-name": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-edit-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-reply-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/navigation": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "700"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--contrast)"
						},
						"typography": {
							"textDecoration": "underline"
						},
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--contrast)"
							},
							"typography": {
								"textDecoration": "none"
							}
						},
						":focus": {
							"color": {
								"text": "var(--wp--preset--color--contrast)"
							}
						},
						":active": {
							"color": {
								"text": "var(--wp--preset--color--contrast)"
							}
						}
					}
				}
			},
			"core/post-author": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--extra-small)"
				}
			},
			"core/post-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--extra-small)"
				}
			},
			"core/post-navigation-link": {
				"typography": {
					"fontWeight": "700"
				}
			},
			"core/post-terms": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--extra-small)"
				}
			},
			"core/post-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"core/query-pagination": {
				"typography": {
					"fontWeight": "700"
				}
			},
			"core/query-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"core/site-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"elements": {
			"button": {
				"border": {
					"color": "var(--wp--preset--color--transparent)",
					"width": "4px",
					"style": "solid",
					"radius": "4px"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--contrast)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "700"
				},
				"shadow": "var(--wp--preset--shadow--natural)",
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--contrast)"
					},
					"border": {
						"color": "var(--wp--preset--color--secondary)"
					}
				}
			},
			"caption": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			},
			"cite": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--contrast)"
					},
					"typography": {
						"textDecoration": "none"
					}
				},
				":focus": {
					"color": {
						"text": "var(--wp--preset--color--contrast)"
					}
				},
				":active": {
					"color": {
						"text": "var(--wp--preset--color--contrast)"
					}
				}
			}
		},
		"spacing": {
			"padding": {
				"right": "var(--wp--preset--spacing--50)",
				"left": "var(--wp--preset--spacing--50)"
			}
		},
		"typography": {
			"fontSize": "var(--wp--preset--font-size--medium)",
			"fontFamily": "var(--wp--preset--font-family--system)",
			"lineHeight": "1.75"
		}
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		}
	],
	"customTemplates": [
		{
			"name": "no-title",
			"title": "No title",
			"postTypes": [ "post","page" ]
		}
	]
}
